function _draw()
	cls(1)
	for y = 0,30,1 do
		if y%2==0 then
			offset=0
		else
			offset=8
		end
		for x = 0,7,1 do
			tile=1
			spr(tile,x*16+offset,y*4)
			spr(tile+1,x*16+offset+8,y*4)
		end
	end
end
